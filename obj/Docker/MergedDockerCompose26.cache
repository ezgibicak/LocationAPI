name: dockercompose1502185083426693581
services:
  locationapi:
    build:
      context: C:\Users\Lenovo\source\repos\ezgibicak\LocationAPI
      dockerfile: LocationAPI/Dockerfile
    container_name: locationapi
    depends_on:
      locationdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      Redis: locationdb:6379
    image: locationapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "7000"
      protocol: tcp
  locationdb:
    container_name: locationdb
    image: redis:alpine
    networks:
      default: null
    ports:
    - mode: ingress
      target: 6379
      published: "6378"
      protocol: tcp
    restart: always
  locationsql:
    environment:
      SA_PASSWORD: 1234!
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1432"
      protocol: tcp
    volumes:
    - type: bind
      source: /path/to/local/dir
      target: /var/opt/mssql
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose1502185083426693581_default