      target: /remote_debugger
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ezgi.bicak\.nuget\packages\
      target: /root/.nuget/packages
      read_only: true
      bind:
        create_host_path: true
    - type: bind
